{
  "name": "scrum-poker",
  "version": "0.4.0",
  "description": "Scrum Poker",
  "main": "node_modules/expo/AppEntry.js",
  "author": {
    "name": "Adam Kruczek",
    "email": "adam.kruczek@briisk.co",
    "url": "https://github.com/akruczek"
  },
  "scripts": {
    "start": "expo start",
    "start:android": "expo start --android",
    "start:ios": "expo start --ios",
    "start:clear": "expo r -c",
    "eject": "expo eject",
    "test": "jest --watch --coverage",
    "test:all": "jest --watchAll --coverage --silent"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|sentry-expo|native-base)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.{model,models}.ts",
      "!src/**/*.spec.{ts,tsx}",
      "!src/**/index.ts",
      "!src/**/constants/*",
      "!src/**/*.styled.{ts,tsx}",
      "!src/**/screens.ts"
    ]
  },
  "dependencies": {
    "@briisk/tslint-config": "^0.1.0",
    "@expo/samples": "2.1.1",
    "@reactivex/rxjs": "^6.5.2",
    "@types/expo": "^32.0.13",
    "@types/expo__vector-icons": "^9.0.1",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.2",
    "@types/ramda": "^0.26.8",
    "@types/react-native": "^0.57.59",
    "@types/react-navigation": "^3.0.7",
    "@types/react-redux": "^7.0.9",
    "@types/react-test-renderer": "^16.8.1",
    "@types/styled-components": "^4.1.15",
    "@types/uuid": "^3.4.5",
    "babel-plugin-module-resolver": "^3.2.0",
    "expo": "^34.0.3",
    "firebase": "^6.3.4",
    "jasmine": "^3.4.0",
    "jasmine-marbles": "^0.6.0",
    "jest": "^24.8.0",
    "metro-react-native-babel-preset": "^0.55.0",
    "ramda": "^0.26.1",
    "react": "16.8.3",
    "react-native": "https://github.com/expo/react-native/archive/sdk-34.0.0.tar.gz",
    "react-native-animatable": "^1.3.2",
    "react-native-base64": "0.0.2",
    "react-native-dotenv": "^0.2.0",
    "react-native-elements": "^1.1.0",
    "react-native-gesture-handler": "^1.2.2",
    "react-native-reanimated": "^1.0.1",
